<%- layout('administrator_layout') %>

<div class="row">
    <div class="col-xs-12">
        <p>&nbsp;</p>
    </div>
</div>

<div class="row">
    <div class="col-xs-4 col-xs-offset-4">
        <div class="panel panel-default">
            <div class="panel-heading">Administrator</div>
            <div class="panel-body">
                <div class="response"></div>
                <form id="js-login-form" enctype="multipart/form-data" method="post">
                    <div class="form-group">
                        <input type="email" class="form-control" name="usermail" id="usermail" required="" value="">
                    </div>
                    <div class="form-group">
                        <input type="password" class="form-control" name="pwd" id="pwd" required="" value="">
                    </div>
                    <input type="submit" class="btn btn-primary" value="Login">
                </form>
            </div>
        </div>
    </div>
</div>
