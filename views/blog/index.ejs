<%- layout('../site/layout') %>

<div class="row">
    <div class="col-xs-8">
        <h1>Art Blog</h1>
    </div>
    <div class="col-xs-4">
        <div class="list-group">
            <% contents.forEach( function (row){ %>
            <a href="/articoli/<%= row.slug %>" class="list-group-item"><%= row.fullname %></a>
            <% }); %>
        </div>
        <div class="panel panel-default">
            <div class="panel-body">
                <%- include ../site/adsbygoogle-300-600.ejs %>
            </div>
        </div>
    </div>
</div>